{
  "name": "New Tab",
  "version": "1.1.4",
  "description": "New Tab!",
  "permissions": [
    "tabs",
    "storage",
    "notifications"
  ],
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "/build/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "/build/cursorEffect.js"
      ]
    }
  ],
  "options_page": "/build/options.html"
}
